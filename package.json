{
  "name": "my-vscode-extension-1",
  "displayName": "Baseline Checker",
  "description": "Check if selected web features are in Baseline (Widely / Newly / Not available).",
  "version": "0.0.2",
  "publisher": "BabuprasanthR",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:baseline.checkSelection",
    "onLanguage:html",
    "onLanguage:css",
    "onLanguage:javascript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "baseline.checkSelection",
        "title": "Baseline: Check Selection"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "baseline.checkSelection",
          "when": "editorHasSelection",
          "group": "navigation"
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Babuprasanth27/my-vscode-extension-1.git"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/node": "18.x",
    "typescript": "^5.0.0",
    "@vscode/test-electron": "^2.3.10"
  },
  "dependencies": {
    "undici": "^6.13.0"
  }
}
